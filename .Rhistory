mod <- lm(y ~ mBmat) #use gibbs to sample coefs in bayes
pred <- predict(mod)
plot(x,y,main=paste0("Figure ",fig," : Univariate Spline Function"))
lines(x, pred, type = "l", lwd = 5, col="blue1")
